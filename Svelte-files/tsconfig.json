{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"compilerOptions": {
	  "moduleResolution": "node",
	  "module": "esnext",
	  "target": "es2019",
	  "isolatedModules": true,
	  "allowJs": true,
	  "checkJs": true,
	  "skipLibCheck": true,
	  "types": ["svelte"]
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.ts", "src/**/*.svelte"],
	"exclude": ["node_modules"]
  }
